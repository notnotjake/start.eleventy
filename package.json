    "dependencies": {
        "@11ty/eleventy": "^2.0.0-beta.1"
    },
	"devDependencies": {
	    "@11ty/eleventy": "^2.0.0-beta.1",
        "npm-run-all": "^4.1.5",
        "sass": "^1.56.1"
	}
}
